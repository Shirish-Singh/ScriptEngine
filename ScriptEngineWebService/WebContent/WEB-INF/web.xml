<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>ScriptEngineWebService</display-name>
  <listener>
    <listener-class>
  		com.sun.xml.ws.transport.http.servlet.WSServletContextListener
  	</listener-class>
  </listener>
  <servlet>
    <servlet-name>ScriptingEngineWS</servlet-name>
    <servlet-class>com.scriptengine.script.servlets.ScriptEngineWSServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>ScriptingEngineWS</servlet-name>
    <url-pattern>/se</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ScriptEngineWSServlet</display-name>
    <servlet-name>ScriptEngineWSServlet</servlet-name>
    <servlet-class>com.scriptengine.script.servlets.ScriptEngineWSServlet</servlet-class>
    <init-param>
      <description>This is a config Path</description>
      <param-name>CONFIG_PATH</param-name>
      <param-value>TBD</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>ScriptEngineWSServlet</servlet-name>
    <url-pattern>/ScriptEngineWSServlet</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>com.scriptengine.script.servlets.listeners.ScriptEngineWSServletListener</listener-class>
  </listener>
</web-app>